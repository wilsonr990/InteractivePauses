<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.interactive.pauses">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ActivitiesActivity"
            android:parentActivityName=".ActivitiesActivity" />
        <activity
            android:name=".JustAnActivity"
            android:parentActivityName=".JustAnActivity" />
        <activity
            android:name=".ProfileActivity"
            android:parentActivityName=".ProfileActivity" />
        <activity
            android:name=".ScheduleActivity"
            android:parentActivityName=".ScheduleActivity" />
        <activity
            android:name=".SettingsActivity"
            android:parentActivityName=".SettingsActivity" />
    </application>

</manifest>